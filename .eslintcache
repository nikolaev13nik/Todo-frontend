        <div className=\"row justify-content-center m-5\">\n                <div className=\"col-12 border\">\n\n                    <div className=\"d-flex mt-3 justify-content-around\">\n                        <div>\n                            <h1>{props.authUser.name} you have {tasks.length} tasks</h1>\n                        </div>\n                        <div>\n                            <button className=\"btn btn-lg  btn-danger\"\n                                    onClick={() => props.changeMainPageAction(\"\")}\n                            >Sign out\n                            </button>\n                        </div>\n                    </div>\n                </div>\n                <div className=\"col-12  mt-5\">\n                    <div className=\"d-flex justify-content-around\">\n                        <Dropdown>\n                            <Dropdown.Toggle variant=\"outline-primary\" id=\"dropdown-basic\">\n                                sort\n                            </Dropdown.Toggle>\n                            <Dropdown.Menu>\n                                <Dropdown.Item onClick={() => handlerSort(\"desDateCreated\")}>sort descending by creation\n                                    date</Dropdown.Item>\n                                <Dropdown.Item onClick={() => handlerSort(\"ascDateCreated\")}>sort ascending by creation\n                                    date</Dropdown.Item>\n                                <Dropdown.Item onClick={() => handlerSort(\"desDateModified\")}>sort descending by\n                                    modified\n                                    date</Dropdown.Item>\n                                <Dropdown.Item onClick={() => handlerSort(\"ascDateModified\")}>sort ascending by modified\n                                    date</Dropdown.Item>\n                            </Dropdown.Menu>\n                        </Dropdown>\n                        <div>\n                            <button className=\"btn btn-sm  btn-outline-primary\"\n                                    onClick={() => props.clearTaskListAction()}>Clear list\n                            </button>\n                        </div>\n                    </div>\n                </div>\n                <div className=\"col-12\">\n                    <Table striped bordered hover className=\"col mt-1\">\n                        <thead>\n                        <tr>\n                            <th>Created date</th>\n                            <th>Modified date</th>\n                            <th className=\"th-title\">Title</th>\n                            <th>Done</th>\n                        </tr>\n                        </thead>\n                        <tbody>\n                        {tasks.map(task => <Task task={task} key={task.id}/>)}\n                        </tbody>\n                    </Table>\n                </div>\n            </div>\n            <div className=\"col-12\">\n                <FormAddTask/>\n            </div>\n\n\n\n        </div>\n\n\n    )\n}\nconst mapDispatchToProps = {\n    changeMainPageAction,\n    loginAction,\n    saveKindSortAction,\n    clearTaskListAction,\n    saveTasksAction,\n    saveUserToStoreAction\n\n\n}\nconst mapStateToProps = (state) => {\n    return {\n        tasks: state.app.tasks,\n        kindSort: state.app.kindSort,\n        authUser: state.user.user\n\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(TodoList)",["86","87"],"/Users/nikitanikolaev/WebstormProjects/react-todo/src/components/FormAddTask.js",[],"/Users/nikitanikolaev/WebstormProjects/react-todo/src/components/SignIn.js",[],"/Users/nikitanikolaev/WebstormProjects/react-todo/src/components/SignUp.js",[],"/Users/nikitanikolaev/WebstormProjects/react-todo/src/components/Task.js",[],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"88","replacedBy":"92"},{"ruleId":"90","replacedBy":"93"},{"ruleId":"88","replacedBy":"94"},{"r